{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.py",
      "use": "@vercel/python"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "api/webhook.py"
    },
    {
      "src": "/api/status",
      "dest": "api/webhook.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/webhook.py"
    },
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ],
  "env": {
    "BOT_TOKEN": "8475765506:AAFENFMSv1Zp9QYMgVnmFonk8I2RJTDQErE"
  }
}